    "spencer-color": "0.1.0",
    "vhtml": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "babelify": "10.0.0",
    "budo": "11.5.0",
    "derequire": "2.0.6",
    "shelljs": "0.8.3",
    "terser": "3.14.1"
  }
}
